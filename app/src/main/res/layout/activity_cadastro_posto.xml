<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="meuposto.br.com.projeto.meuposto.activity.CadastroPostoActivity"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">


    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:text="@string/nome_posto"
        android:textColor="@android:color/background_dark"
        android:textSize="24sp"
        android:layout_height="wrap_content"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="14dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/endere_posto"
        android:textColor="@android:color/background_dark"
        android:textSize="24sp"
        android:layout_marginStart="16dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/enderecoPostoId"
        android:layout_marginLeft="16dp" />

    <TextView
        android:id="@+id/precosId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="@string/pre_os"
        android:textColor="@android:color/background_dark"
        android:textSize="24sp"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="@string/localiza_o"
        android:textColor="@android:color/background_dark"
        android:textSize="24sp"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="27dp"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="@string/bandeira_posto"
        android:textColor="@android:color/background_dark"
        android:textSize="24sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/bandeiraPosrtoId"
        android:layout_marginLeft="16dp" />

    <TextView
        android:id="@+id/bandeiraPosrtoId"
        android:layout_width="0dp"
        android:layout_height="27dp"
        android:hint="@string/digite_a_bandeira"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/selecione_a_bandeira"
        android:textSize="18sp"

        android:layout_marginStart="16dp"
        tools:ignore="TextViewEdits,UnknownId"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="26dp"
        app:layout_constraintTop_toBottomOf="@+id/enderecoPostoId"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/textView10"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp" />

    <EditText
        android:hint="@string/digite_o_nome"
        android:id="@+id/nomePostoId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/enderecoPostoId"
        android:layout_marginTop="74dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/enderecoPostoId"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/enderecoPostoId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="@string/digite_o_endereco"
        android:inputType="textPersonName"
        tools:ignore="UnknownId"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/bandeiraPosrtoId"
        android:layout_marginTop="14dp"
        app:layout_constraintTop_toBottomOf="@+id/nomePostoId"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/bandeiraPosrtoId" />


    <Button
        android:id="@+id/botaoCadastrarPostoId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/cadastrar_posto"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/imgPrecosId"
        android:layout_marginTop="13dp"
        app:layout_constraintTop_toBottomOf="@+id/imgPrecosId"
        android:layout_marginRight="35dp" />

    <ImageButton
        android:id="@+id/igmMapId"
        android:layout_width="81dp"
        android:layout_height="36dp"
        app:srcCompat="@android:drawable/ic_dialog_map"
        android:contentDescription=""
        tools:ignore="ContentDescription"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/botaoCadastrarPostoId"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="124dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="125dp"
        app:layout_constraintLeft_toLeftOf="@+id/bandeiraPosrtoId"
        app:layout_constraintTop_toTopOf="@+id/enderecoPostoId"
        android:layout_marginLeft="75dp"
        android:layout_marginRight="77dp" />

    <ImageButton
        android:id="@+id/imgPrecosId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_menu_edit"
        android:contentDescription=""
        tools:ignore="ContentDescription"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/igmMapId"
        android:layout_marginLeft="10dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline"
        app:layout_constraintGuide_begin="20dp"
        android:orientation="horizontal"
        tools:layout_editor_absoluteY="20dp"
        tools:layout_editor_absoluteX="0dp" />
</android.support.constraint.ConstraintLayout>
